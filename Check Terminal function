    def check_terminal(self):
            sum=0;
            for r in range(size):
                for c in range(size-1):
                    sum +=self.horizontal[r][c]
            for r in range(size-1):
                for c in range(size):
                    sum +=self.vertical[r][c]    
            if sum==(size*(size-1)*2):
                return 1
            else:
                return 0
